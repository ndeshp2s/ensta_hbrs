<?xml version="1.0"?>
<launch>

  	<!-- these are the arguments you can pass this launch file, for example 
		paused:=true -->
	<arg name="paused" default="false" />
	<arg name="use_sim_time" default="true" />
	<arg name="gui" default="true" />
	

	<!-- start gazebo and load environment-->
	<include file="$(find ensta_gazebo_worlds)/launch/world-1.launch">
        	<arg name="paused" value="$(arg paused)" />
       	        <arg name="use_sim_time" value="$(arg use_sim_time)" />
        	<arg name="gui" value="$(arg gui)" />
   	</include>


    	<!-- upload robot description >
	<include file="$(find p3dx_gazebo)/launch/gazebo.launch" /-->
    	

	<!-- Select the robot-driver you wish to use>
	<include file="$(find p2os_urdf)/launch/pioneer3dx.gazebo.launch" / -->

	<include file="$(find p2os_urdf)/launch/upload_pioneer3dx.xml"/>
	<node name="spawn_pioneer" pkg="gazebo_ros" type="spawn_model" args="-z 0.1 -urdf -param robot_description -model robot_description" respawn="false" output="screen"/>

	<!-- Launch Applications  -->
	  <include file="$(find pioneer3at)/launch/apps/gmapping.launch" />
	  <include file="$(find pioneer3at)/launch/apps/navigation.launch" />
	  
	  <!-- Launch Visualization  -->
	  <include file="$(find pioneer3at)/launch/ui.launch" />


</launch>
